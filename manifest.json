{
  "manifest_version": 2,
  "name": "ASKA Copy clipboard.",
  "description": "Copy clipboard programmatically at any time.",
  "version": "1.0",
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite"
  ],
  "externally_connectable": {
    "matches": [
      "https://192.168.2.100:4444/*",
      "https://192.168.2.101:4444/*",
      "https://192.168.2.102:4444/*",
      "https://192.168.2.103:4444/*",
      "https://192.168.2.104:4444/*",
      "https://192.168.2.105:4444/*"
    ]
  }
}
